{
  "name": "newman-parallel",
  "version": "1.0.9",
  "description": "A framework to execute multiple Postman collections in parallel run with Newman.Includes Allure, HTML-extra and Junit reporters integrations",
  "main": "index.js",
  "scripts": {
    "test": "node index.js test/col test/env ALL E=MyEnvironment",
    "lint": "npx eslint ./src/*.js --fix",
    "format": "prettier --write ./src/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(cjs|js|ts|tsx)": [
      "eslint --fix"
    ],
    "**/*.+(cjs|js|ts|json)": [
      "prettier --write"
    ]
  },
  "keywords": [
    "allure",
    "newman",
    "reporter",
    "reports",
    "api",
    "postman",
    "tests"
  ],
  "author": "Valentin Lutchanka",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Valiantsin2021/Newman-parallel-run"
  },
  "bin": {
    "newman-parallel": "./index.js"
  },
  "dependencies": {
    "@felipecrs/newman-reporter-allure": "1.1.2",
    "allure-commandline": "2.27.0",
    "allure-patch": "^1.0.3",
    "async": "^3.2.5",
    "newman": "6.1.0",
    "newman-reporter-htmlextra": "1.23.0",
    "newman-reporter-junitfull": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "^4.3.8",
    "prettier": "3.2.4"
  }
}